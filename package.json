{
  "name": "react-base-code",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Vijay Dev <vijaydev.in@gmail.com> (http://vijaydev.com/)",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iamvijaydev/react-base-code.git"
  },
  "license": "MIT",
  "scripts": {
    "setup": "npm install",
    "test": "jest --colors",
    "test:u": "echo \"updating snapshots...\" && jest -u --colors",
    "langs": "npx babel-node build/intl/translator.js",
    "dist": "npm run langs && webpack -p --env.env=prod",
    "start": "npm run langs && webpack-dev-server --env.env=dev"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "base-href-webpack-plugin": "^2.0.0",
    "prop-types": "^15.6.1",
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "react-helmet": "^5.2.0",
    "react-intl-universal": "^1.11.4",
    "react-loadable": "^5.3.1",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "styled-components": "^3.2.5"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-jest": "^22.4.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-dynamic-import-node": "^1.2.0",
    "babel-plugin-import-inspector": "^2.0.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "chalk": "^2.3.2",
    "clean-webpack-plugin": "^0.1.19",
    "copy-webpack-plugin": "^4.5.1",
    "css-loader": "^0.28.11",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.10.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.4.0",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "import-inspector": "^2.0.0",
    "jest": "^22.4.3",
    "react-hot-loader": "^4.0.1",
    "style-loader": "^0.21.0",
    "uglifyjs-webpack-plugin": "^1.2.4",
    "webpack": "^4.5.0",
    "webpack-cli": "^2.0.14",
    "webpack-dev-server": "^3.1.3",
    "webpack-merge": "^4.1.2",
    "write-file-webpack-plugin": "^4.2.0"
  },
  "jest": {
    "verbose": true,
    "collectCoverage": false,
    "collectCoverageFrom": [
      "**/*.js",
      "!webpack.config.js",
      "!**/node_modules/**",
      "!**/config/**",
      "!**/build/**",
      "!**/coverage/**",
      "!**/dist/**"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 70,
        "functions": 70,
        "lines": 70,
        "statements": 70
      }
    },
    "moduleDirectories": [
      "node_modules"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/buld/jest/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "setupTestFrameworkScriptFile": "<rootDir>/build/jest/setupTest.js"
  }
}
